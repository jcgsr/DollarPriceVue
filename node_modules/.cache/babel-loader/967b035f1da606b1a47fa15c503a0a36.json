n    <v-input>\n      <v-text-field type=\"number\" v-model=\"moedaA_value\" :placeholder=\"moedaA\"></v-text-field>\n    </v-input>\n    <v-btn id=\"btn\" color=\"success\" @click=\"converter\">Converter</v-btn>\n    <h2 id=\"value\">{{ moedaB_value | currency }}</h2>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"Conversor\",\n  props: [\"moedaA\", \"moedaB\"],\n  data() {\n    return {\n      moedaA_value: \"\",\n      moedaB_value: 0,\n    };\n  },\n\n  methods: {\n    converter() {\n      let de_para = this.moedaA + \"_\" + this.moedaB;\n      let url =\n        \"https://free.currconv.com/api/v7/convert?q=\" +\n        de_para +\n        \"&compact=ultra&apiKey=41c3d3eae7092993a692\";\n\n      fetch(url)\n        .then((res) => {\n          return res.json();\n        })\n        .then((json) => {\n          let cotacao = json[de_para];\n          this.moedaB_value = (cotacao * parseFloat(this.moedaA_value)).toFixed(\n            2\n          );\n        });\n    },\n  },\n  filters: {\n    currency(value) {\n\t\t if (value == this.moedaB) {\n\n\t\t\t return \"R$ \" + value.toLocaleString();\n\t\t }\n    },\n  },\n};\n</script>\n\n<style>\n#btn {\n  color: aliceblue;\n}\n\n#value {\n  margin-top: 1rem;\n}\n</style>\n"],"sourceRoot":"src/components"}]}